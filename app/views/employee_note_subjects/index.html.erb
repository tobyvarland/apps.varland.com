<p id="notice"><%= notice %></p>

<h1>Employee Note Subjects</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Employee note</th>
      <th>Note type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_note_subjects.each do |employee_note_subject| %>
      <tr>
        <td><%= employee_note_subject.user_id %></td>
        <td><%= employee_note_subject.employee_note_id %></td>
        <td><%= employee_note_subject.note_type %></td>
        <td><%= link_to 'Show', employee_note_subject %></td>
        <td><%= link_to 'Edit', edit_employee_note_subject_path(employee_note_subject) %></td>
        <td><%= link_to 'Destroy', employee_note_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee Note Subject', new_employee_note_subject_path %>
