<p id="notice"><%= notice %></p>

<h1>Groov Bake Shop Orders</h1>

<table>
  <thead>
    <tr>
      <th>Cycle</th>
      <th>AS400 shop order</th>
      <th>Container type</th>
      <th>Operation letter</th>
      <th>Bake profile</th>
      <th>Setpoint</th>
      <th>Minimum</th>
      <th>Maximum</th>
      <th>Soak length</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groov_bake_shop_orders.each do |groov_bake_shop_order| %>
      <tr>
        <td><%= groov_bake_shop_order.cycle_id %></td>
        <td><%= groov_bake_shop_order.as400_shop_order_id %></td>
        <td><%= groov_bake_shop_order.container_type %></td>
        <td><%= groov_bake_shop_order.operation_letter %></td>
        <td><%= groov_bake_shop_order.bake_profile %></td>
        <td><%= groov_bake_shop_order.setpoint %></td>
        <td><%= groov_bake_shop_order.minimum %></td>
        <td><%= groov_bake_shop_order.maximum %></td>
        <td><%= groov_bake_shop_order.soak_length %></td>
        <td><%= link_to 'Show', groov_bake_shop_order %></td>
        <td><%= link_to 'Edit', edit_groov_bake_shop_order_path(groov_bake_shop_order) %></td>
        <td><%= link_to 'Destroy', groov_bake_shop_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Groov Bake Shop Order', new_groov_bake_shop_order_path %>
