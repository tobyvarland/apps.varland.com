<p id="notice"><%= notice %></p>

<h1>Groov Baking Shop Orders</h1>

<table>
  <thead>
    <tr>
      <th>Cycle</th>
      <th>Shop order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groov_baking_shop_orders.each do |groov_baking_shop_order| %>
      <tr>
        <td><%= groov_baking_shop_order.cycle_id %></td>
        <td><%= groov_baking_shop_order.shop_order_id %></td>
        <td><%= link_to 'Show', groov_baking_shop_order %></td>
        <td><%= link_to 'Edit', edit_groov_baking_shop_order_path(groov_baking_shop_order) %></td>
        <td><%= link_to 'Destroy', groov_baking_shop_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

