<p id="notice"><%= notice %></p>

<h1>Quality Calibration Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Calibration frequency</th>
      <th>Instructions url</th>
      <th>Two point low value</th>
      <th>Two point high value</th>
      <th>Calculate offset and gain</th>
      <th>Require offset</th>
      <th>Require gain</th>
      <th>Enable notifications</th>
      <th>Discarded at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quality_calibration_categories.each do |quality_calibration_category| %>
      <tr>
        <td><%= quality_calibration_category.name %></td>
        <td><%= quality_calibration_category.calibration_frequency %></td>
        <td><%= quality_calibration_category.instructions_url %></td>
        <td><%= quality_calibration_category.two_point_low_value %></td>
        <td><%= quality_calibration_category.two_point_high_value %></td>
        <td><%= quality_calibration_category.calculate_offset_and_gain %></td>
        <td><%= quality_calibration_category.require_offset %></td>
        <td><%= quality_calibration_category.require_gain %></td>
        <td><%= quality_calibration_category.enable_notifications %></td>
        <td><%= quality_calibration_category.discarded_at %></td>
        <td><%= link_to 'Show', quality_calibration_category %></td>
        <td><%= link_to 'Edit', edit_quality_calibration_category_path(quality_calibration_category) %></td>
        <td><%= link_to 'Destroy', quality_calibration_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quality Calibration Category', new_quality_calibration_category_path %>
